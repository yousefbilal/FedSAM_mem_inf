    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/models",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "-dataset cifar10 --num-rounds 10000 --eval-every 100 --batch-size 64 --num-epochs 1 --clients-per-round 5 -model cnn -lr 0.01 --weight-decay 0.0004 -device cuda:0 -algorithm fedopt --server-lr 1 --server-opt sgd --num-workers 0 --where-loading init -alpha 0.5"
        }
    ]
}